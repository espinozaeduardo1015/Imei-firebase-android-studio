<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.imeiinfopy.imeiinfopy">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="26" />
    <meta-data android:name="android.webkit.WebView.EnableSafeBrowsing"
        android:value="true" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login.ResetPasswordActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login.SignupActivity"
            android:label="@string/title_activity_signup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".perfile.EditarPerfil"
            android:label="@string/title_activity_editar_perfil"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".perfile.SignOut"
            android:label="@string/title_activity_sign_out"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActividadActivity"
            android:label="Verificacion de Seguridad"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ConatelP"
            android:label="CONATEL Â© 2017"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Registro_imei"
            android:label="Registra tu IMEI"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Dispositivos"
            android:label="@string/title_activity_dispositivos"
            android:parentActivityName=".MainActivity">


        </activity>
        <activity
            android:name=".LocalizarDispositivo"
            android:label="Localizar Dispositivos"
            android:parentActivityName=".MainActivity">
            <meta-data android:name="android.webkit.WebView.EnableSafeBrowsing"
                android:value="true" />
        </activity>
        <activity android:name=".PreguntasActivity"
            android:theme="@style/AppTheme.NoActionBar"
            >

        </activity>
    </application>

</manifest>